{
  "name": "devrantron",
  "version": "1.5.2",
  "main": "./build/app/app.js",
  "description": "An open source cross platform desktop application for devRant",
  "scripts": {
    "lint": "eslint --ext .jsx,.js app/src/js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress --color --config ./webpack/prod.config.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint-fix": "eslint --ext .jsx,.js app/src/js --fix",
    "publish": "electron-builder --draft",
    "postinstall": "electron-builder install-app-deps",
    "dev": "cross-env NODE_ENV=development concurrently -k \"npm run server\" \"npm run start\" ",
    "server": "webpack-dev-server --color --config webpack/dev.config.js",
    "start": "cross-env NODE_ENV=development electron app/src/app.js",
    "start-build": "electron build/app/app.js",
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "profile": "webpack --config webpack/prod.config.js --profile --json > stats.json"
  },
  "license": "Apache-2.0",
  "build": {
    "productName": "devRantron",
    "appId": "com.tahnik.devrantron",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/app"
    ]
  },
  "author": {
    "name": "Tahnik Mustasin",
    "email": "tahnik@live.co.uk",
    "url": "https://www.tahnik.com/"
  },
  "contributors": [
    {
      "name": "Hampus Lundqvist Norman",
      "url": "https://rekkyrek.se"
    },
    {
      "name": "Sven Lechner",
      "url": "https://sirwindfield.github.io"
    }
  ],
  "keywords": [
    "Electron",
    "react"
  ],
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^1.1.3",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.9",
    "cz-conventional-changelog": "^2.1.0",
    "electron": "^1.7.11",
    "electron-builder": "^19.55.2",
    "env-paths": "^1.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.0",
    "react-hot-loader": "^3.1.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "autolinker": "^1.6.0",
    "deepmerge": "^2.0.1",
    "dompurify": "^1.0.3",
    "electron-devtools-installer": "^2.2.3",
    "electron-settings": "^3.1.4",
    "electron-updater": "^2.20.1",
    "fuse.js": "^3.2.0",
    "jquery": "^3.3.1",
    "json-schema-traverse": "^0.3.1",
    "node-notifier": "^5.2.1",
    "prop-types": "^15.6.0",
    "rantscript": "^1.4.0",
    "react": "^16.2.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.1",
    "react-twemoji": "0.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semver-compare": "^1.0.0",
    "showdown": "^1.8.6",
    "twemoji": "^2.5.0",
    "webpack-sources": "^1.1.0"
  },
  "optionalDependencies": {
    "7zip-bin-linux": "^1.3.1",
    "7zip-bin-mac": "^1.0.1",
    "7zip-bin-win": "^2.1.1"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint-fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "__tests__/utils"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  }
}
