{
  "name": "devrantron",
  "version": "0.0.1",
  "main": "./app/build/app.js",
  "description": "An open source cross platform desktop application for devRant",
  "scripts": {
    "clear": "rimraf dist",
    "ci:all": "npm-run-all ci:lint ci:test ci:coverage ci:build ci:package ci:deploy",
    "ci:build": "webpack --progress --color --config ./configs/webpack.prod.config.js",
    "ci:coverage": "echo ci:coverage",
    "ci:deploy": "echo ci:deploy",
    "ci:lint": "echo ci:lint",
    "ci:package": "echo ci:package",
    "ci:test": "npm run test -- --runInBand",
    "contribute": "npm-run-all lint test",
    "dev": "concurrently -k \"npm run server\" \"npm run start\" ",
    "lint": "eslint --ext .jsx,.js app/src/main",
    "package:all": "",
    "package:dir": "build --dir",
    "package:lin": "",
    "package:mac": "",
    "package:win": "",
    "package": "build",
    "server": "webpack-dev-server --color --config configs/webpack.dev.config.js",
    "start": "cross-env NODE_ENV=development electron app/src/main/app.js",
    "start:app": "electron app/build/app.js",
    "test": "jest -c configs/jest.json"
  },
  "build": {
    "productName": "devRantron",
    "appId": "de.zerotask.theclassic",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "squirrel"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "output": "release"
    }
  },
  "author": {
    "name": "Tahnik Mustasin",
    "email": "tahnik@live.co.uk",
    "url": "https://www.tahnik.com/"
  },
  "contributors": [
    {
      "name": "Hampus Lundqvist Norman",
      "url": "https://rekkyrek.se"
    },
    {
      "name": "Sven Lechner",
      "url": "https://sirwindfield.github.io"
    }
  ],
  "keywords": [
    "Electron",
    "react"
  ],
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "electron-builder": "^16.8.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "redux-logger": "^3.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "electron-devtools-installer": "^2.2.0",
    "jquery": "^3.2.1",
    "rantscript": "^1.0.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "twemoji": "^2.2.5"
  }
}
